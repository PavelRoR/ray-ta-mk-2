OS = 'OpenSans', sans-serif
OSB = 'OpenSansBold', sans-serif
C = 'Circe', sans-serif
CB = 'CirceBold', sans-serif

@font-face {
	font-family: 'OpenSans';
	src: url('../fonts/OpenSans.eot');
	src: url('../fonts/OpenSans.eot') format('embedded-opentype'),
		url('../fonts/OpenSans.woff2') format('woff2'),
		url('../fonts/OpenSans.woff') format('woff'),
		url('../fonts/OpenSans.ttf') format('truetype'),
		url('../fonts/OpenSans.svg#OpenSans') format('svg');
	font-display: fallback;
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'OpenSansBold';
	src: url('../fonts/OpenSansBold.eot');
	src: url('../fonts/OpenSansBold.eot') format('embedded-opentype'),
		url('../fonts/OpenSansBold.woff2') format('woff2'),
		url('../fonts/OpenSansBold.woff') format('woff'),
		url('../fonts/OpenSansBold.ttf') format('truetype'),
		url('../fonts/OpenSansBold.svg#OpenSansBold') format('svg');
	font-display: fallback;
	font-weight: bold
	font-style: normal;
}
@font-face {
	font-family: 'Circe';
	src: url('../fonts/Circe.eot');
	src: url('../fonts/Circe.eot') format('embedded-opentype'),
		url('../fonts/Circe.woff2') format('woff2'),
		url('../fonts/Circe.woff') format('woff'),
		url('../fonts/Circe.ttf') format('truetype'),
		url('../fonts/Circe.svg#Circe') format('svg');
	font-display: fallback;
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'CirceBold';
	src: url('../fonts/CirceBold.eot');
	src: url('../fonts/CirceBold.eot') format('embedded-opentype'),
		url('../fonts/CirceBold.woff2') format('woff2'),
		url('../fonts/CirceBold.woff') format('woff'),
		url('../fonts/CirceBold.ttf') format('truetype'),
		url('../fonts/CirceBold.svg#CirceBold') format('svg');
	font-display: fallback;
	font-weight: bold
	font-style: normal;
}
@import '../css/_css/bootstrap.min.css'
@import '../css/_css/intlTelInput.min.css'

*
	margin 0
	padding 0
	outline none
body
	font-size: 16px
	font-family: C
	color #000
	background-color #fff
	line-height: 20px
a
	color: initial
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	margin 0 auto
	width 100%
	max-width: 263px;
	height: 52px;
	line-height: 52px
	padding 0 5px
	background-color: #4564d3
	font-family: CB
	text-align: center;
	color #fff
	border-radius: 15px
	&:focus
		color #fff
		background-color: #4564d3
	&:hover
		opacity .86
.general-title
	font-size: 35px;
	font-family: CB
	line-height: 0.743;
	// text-align: left
	margin-bottom: 40px
@import 'main/0.header'
@import 'thanks/1.main'
@import 'bonus/3.footer'
@import 'thanks/2.medias'